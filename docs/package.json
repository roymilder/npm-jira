{
  "name": "npm-jira",
  "version": "0.0.0",
  "description": "Create JIRA issues from the command line!",
  "main": "index.js",
  "scripts": {
    "debug": "npm build &&  node dist/index.js",
    "copy": "cp src/config.json dist/",
    "test": "npm run lint && mocha && npm run docs-build",
    "build": "rm -rf dist && babel src --out-dir dist && npm run copy",
    "docs-build": "rm -rf docs && esdoc -c esdoc.json",
    "lint": "eslint ./",
    "prepublish": "npm run build && npm run docs-build",
    "release": "release"
  },
  "bin": {
    "jira": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roymilder/npm-jira.git"
  },
  "keywords": [
    "jira",
    "npm",
    "client"
  ],
  "author": "Roy Milder",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/roymilder/npm-jira/issues"
  },
  "homepage": "https://github.com/roymilder/npm-jira#readme",
  "dependencies": {
    "inquirer": "^3.0.1",
    "jira-client": "^4.2.0",
    "prettyjson": "^1.2.1",
    "prompt": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "esdoc": "^0.4.3",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "^2.6.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-mocha": "^2.0.0",
    "mocha": "^2.3.4",
    "release-script": "^1.0.1"
  }
}
